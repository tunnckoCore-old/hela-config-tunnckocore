{
  "name": "hela-config-tunnckocore",
  "version": "2.0.23",
  "description": "A [hela][] task runner shareable config (preset of tasks) for @tunnckoCore org",
  "repository": "tunnckoCore/hela-config-tunnckocore",
  "author": "Charlike Mike Reagent (https://charlike.online)",
  "nspId": "e5980075-9545-47a2-b946-35ee5ba2091d",
  "main": "./src/index.js",
  "scripts": {
    "start": "hela",
    "test": "yarn start test",
    "precommit": "yarn start precommit",
    "commit": "yarn start commit"
  },
  "license": "Apache-2.0",
  "licenseStart": 2017,
  "engines": {
    "node": ">=6",
    "npm": ">=5",
    "yarn": ">=1"
  },
  "files": [
    "src/"
  ],
  "dependencies": {
    "all-contributors-cli": "4.6.0",
    "eslint": "4.10.0",
    "eslint-config-tunnckocore": "0.2.0",
    "gitcommit": "0.2.4",
    "hela": "1.3.14",
    "is-ci": "1.0.10",
    "nyc": "11.3.0",
    "parse-git-log": "0.2.1",
    "prettier": "1.8.1",
    "prettier-eslint": "8.2.1",
    "prettier-eslint-cli": "4.4.0",
    "rimraf": "2.6.2"
  },
  "devDependencies": {
    "mukla": "0.4.9"
  },
  "keywords": [
    "automation",
    "hela",
    "hela-config",
    "shareable config",
    "tasks",
    "tunnckocore",
    "preset",
    "hela-preset"
  ],
  "verb": {
    "run": true,
    "toc": {
      "render": true,
      "method": "preWrite",
      "maxdepth": 3
    },
    "layout": "empty",
    "tasks": [
      "readme"
    ],
    "related": {
      "list": [
        "gitcommit",
        "p-map-series",
        "execa",
        "hela",
        "mri"
      ],
      "highlight": "hela"
    },
    "lint": {
      "reflinks": true
    },
    "reflinks": []
  }
}
