      "babel-preset-env",
      "charlike-cli",
      "eslint",
      "execa",
      "hela",
      "nyc",
      "prettier",
      "rimraf",
      "rollup",
      "rollup-plugin-babel",
      "verb",
      "semantic-release",
      "simple-commit-message"
